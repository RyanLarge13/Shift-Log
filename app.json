    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2b085e"
      },
      "package": "com.ryanlarge.ShiftLog"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8f6e815c-59d8-4f1d-8b1f-bcca88dd6c0d"
      }
    }
  }
}
